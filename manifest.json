{
    "name": "Omnivox Dark Theme",
    "version": "0.0.4",
    "manifest_version": 3,
    "description": "Omnivox Dark Theme by majeurbilly",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "content_scripts": [
      {
        "matches": ["https://*.omnivox.ca/*"],
        "css": ["dist/inject/inject.css"],
        "js": ["dist/inject/callbacks.js", "dist/inject/darkMode.js", "dist/inject/inject.js"],
        "run_at": "document_start",
        "exclude_matches": ["https://*.omnivox.ca/cvir/ddle/VisualiseDocument.aspx*"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["dist/inject/icon.svg"],
        "matches": ["https://*.omnivox.ca/*"]
      }
    ],
    "permissions": [
      "storage"
    ],
    "host_permissions": [
      "https://*.omnivox.ca/*"
    ]
  }